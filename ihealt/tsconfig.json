{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/screens/*": [
        "./src/screens/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ]
    }
  }
}